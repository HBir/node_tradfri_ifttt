{
    "extends": "airbnb-base",
    "rules": {
      "no-console": 0,
      "strict": [
        0,
        "global"
      ],
      "import/no-extraneous-dependencies": 0,
      "no-underscore-dangle": 0,
      "global-require": 0,
      "no-unused-expressions": 0,
      "implicit-arrow-linebreak": 0,
      "max-len": [1, 140, 4]
    },
    "env": {
      "node": true,
      "mocha": true
    }
  }